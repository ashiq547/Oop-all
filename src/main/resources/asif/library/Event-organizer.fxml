<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="asif.library.EventOrganizer">
   <children>
      <Button layoutX="32.0" layoutY="35.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="←">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Label layoutX="228.0" layoutY="37.0" text="Event Organizer">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button layoutX="482.0" layoutY="32.0" mnemonicParsing="false" onAction="#onLogOutButtonClick" prefHeight="40.0" prefWidth="66.0" text="LogOut">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="32.0" layoutY="128.0" mnemonicParsing="false" onAction="#ongoToOrganizerSheduleEvents" prefHeight="52.0" prefWidth="126.0" text="Shedule New Events" />
      <Button layoutX="215.0" layoutY="128.0" mnemonicParsing="false" onAction="#ongoToEventRegistration" prefHeight="52.0" prefWidth="126.0" text="Event Registration" wrapText="true" />
      <Button layoutX="215.0" layoutY="215.0" mnemonicParsing="false" onAction="#ongoToCancelEvents" prefHeight="52.0" prefWidth="126.0" text="Cancel Events" />
      <Button layoutX="137.0" layoutY="295.0" mnemonicParsing="false" onAction="#ongoToEventReports" prefHeight="52.0" prefWidth="126.0" text="Event Reports" />
      <Button layoutX="389.0" layoutY="128.0" mnemonicParsing="false" onAction="#ongoToEventNotifyParticipants" prefHeight="52.0" prefWidth="158.0" text="Notify Event Participants" />
      <Button layoutX="32.0" layoutY="215.0" mnemonicParsing="false" onAction="#ongoToUpdateEventDetails" prefHeight="52.0" prefWidth="126.0" text="Update Event Details" />
      <Button layoutX="397.0" layoutY="215.0" mnemonicParsing="false" onAction="#ongoToUpcomingEvents" prefHeight="52.0" prefWidth="126.0" text="Upcoming Events" />
      <Button layoutX="312.0" layoutY="295.0" mnemonicParsing="false" onAction="#ongoToFeedBack" prefHeight="52.0" prefWidth="116.0" text="Feedback" />
   </children>
</AnchorPane>
